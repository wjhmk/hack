<h1>Questions#index</h1>
<p>Find me in app/views/questions/index.html.erb</p>

<p><%= link_to "質問する" , new_question_path  %></p>


<h1>質問一覧</h1>
<% @questions.each do |question|%>
#######################
<p><%= question.id %></p>
<p>タイトル：<%= link_to question.title , question %></p>
